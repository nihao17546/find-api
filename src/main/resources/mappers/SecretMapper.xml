<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.appcnd.find.api.dao.ISecretDAO">
    <select id="selectByName" resultType="com.appcnd.find.api.pojo.po.SecretPO">
        select * from `tb_secret` where `name`=#{name}
    </select>
    <select id="selectAll" resultType="com.appcnd.find.api.pojo.po.SecretPO">
        select * from `tb_secret`
    </select>
</mapper>